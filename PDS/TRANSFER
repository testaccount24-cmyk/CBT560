//BC0THORF JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=4M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//*******************************************************************
//*                 BC0THOR.PDS.JCL(TRANSFER)                       *
//* EXCHANGE, RENAME, ETC TO TRANSFER NEW SOFTWARE TO PRODUCTION.   *
//*******************************************************************
//*
//SCRATCH  EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 DELETE BC0THOR.TST.EXCHANGE.SYSLOG NONVSAM
//*
//EXCHANGE EXEC  PGM=EXCHANGE
//STEPLIB  DD    DSN=SYS1.SYSA.HNET.LINKLIB,DISP=SHR
//SYSUDUMP DD    SYSOUT=*
//SYSPRINT DD    SYSOUT=*
//SYSLOG   DD    DSN=BC0THOR.TST.EXCHANGE.SYSLOG,DISP=(,CATLG),
//         UNIT=DISK,SPACE=(TRK,(10,2),RLSE),
//         RECFM=FB,LRECL=133,BLKSIZE=0
//SYSIN    DD    *
VOLUME SYSRES
EXCHANGE DSN=SYS1.LPALIB
         DSN=SYS5.LPALIB
VOLUME SYSRES
EXCHANGE DSN=SYS1.SYSA.HNET.LINKLIB
         DSN=SYS5.SYSA.HNET.LINKLIB
//*******************************************************************
//*      RENAME RESULTING SYS5.* DATASETS TO SYS9.*                 *
//*******************************************************************
//IEHPROGM EXEC  PGM=IEHPROGM
//SYSPRINT DD  SYSOUT=*
//DD1      DD  UNIT=DISK,VOL=SER=SYSRES,DISP=OLD
//DD2      DD  UNIT=DISK,VOL=SER=SYSRES,DISP=OLD
//SYSIN DD *
 RENAME DSNAME=SYS5.SYSA.HNET.LINKLIB,VOL=DISK=SYSRES,                 X
               NEWNAME=SYS9.SYSA.HNET.LINKLIB
 RENAME DSNAME=SYS5.LPALIB,VOL=DISK=SYSRES,                            X
               NEWNAME=SYS9.LPALIB
