//SCRATCH  EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 DELETE BC0THOR.SP4RES.LISTCAT NONVSAM
//*
//*
//LISTCAT  EXEC PGM=IDCAMS
//SYSPRINT DD DSN=BC0THOR.SP4RES.LISTCAT,DISP=(,CATLG),
//         UNIT=DISK,SPACE=(TRK,(15,5),RLSE)
//SYSIN    DD *
 LISTCAT CATALOG(SYS1.CAT.SP4RES) VOL
