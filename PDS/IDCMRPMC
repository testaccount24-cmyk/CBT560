//BC0THORG JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=4M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//*********************************************************************
//*        BC0THOR.PDS.JCL(IDCMRPMC)                                  *
//*  IDCAMS VSAM FILE REPRO - MERGECAT TO FINISH A RENAME FOR A FILE  *
//*  RENAMED WITH A NEW HIGH LEVEL QUALIFIER IN A CATALOG DIFFERENT   *
//*  FROM THE ORIGINAL CATALOG. THE RENAME ALONE DOES NOT RECATALOG.  *
//*********************************************************************
//REPROMC  EXEC PGM=IDCAMS,REGION=2048K
//SYSPRINT DD   SYSOUT=*
//SYSUDUMP DD   SYSOUT=*
//DD1      DD   UNIT=DISK,VOL=SER=MVS220,DISP=OLD
//OLDCAT   DD   DSN=SYS1.CAT.TSO,DISP=SHR
//NEWCAT   DD   DSN=SYS1.CAT.SYSTEM,DISP=SHR
//SYSIN    DD   *
 REPRO -
    MERGECAT -
    INFILE(OLDCAT) -
    OUTFILE(NEWCAT) -
    ENTRIES(SPP.VSAMNAME) -
    FILE(DD1)
