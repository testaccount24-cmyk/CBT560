//BC0THORJ JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=4M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//*******************************************************************
//*                 BC0THOR.PDS.JCL(CATALOGD)                       *
//* FIRST SORTS FILE OF DSNAME/VOLSER INFO IN ASCENDING A/N ORDER,  *
//* THEN CHECKS EACH DSNAME AGAINST THE CATALOG. IF THE DSNAME IS   *
//* CATALOGED ON THE GIVEN VOLSER THE INPUT RECORD IS WRITTEN TO    *
//* THE OUTPUT1 FILE, OTHERWISE TO THE PRINT1 FILE.                 *
//*******************************************************************
//*
//SCRATCH  EXEC PGM=IDCAMS,REGION=2M
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 DELETE BC0THOR.TST.SORTED
 DELETE BC0THOR.TST.CATALOGD
//*
//SORT     EXEC PGM=SORT33,PARM='CORE=MAX,MSG=AP'
//SYSUDUMP DD SYSOUT=(*,,DUMP)
//SORTIN   DD DSN=BC0THOR.TST.FILES,DISP=SHR
//SORTOUT  DD DSN=BC0THOR.TST.SORTED,DISP=(,CATLG),
//         UNIT=DISK,SPACE=(TRK,(15,5),RLSE),
//         DSORG=PS,RECFM=FB,LRECL=80
//SORTWK01 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK02 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK03 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK04 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK05 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK06 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SYSOUT   DD SYSOUT=*
//SYSIN    DD *
 SORT FIELDS=(1,51,CH,A)
 SUM FIELDS=NONE
//*
//CATALOGD EXEC PGM=CATALOGD
//STEPLIB  DD   DSN=BC0THOR.LOAD.LOAD,DISP=SHR
//SYSUDUMP DD   SYSOUT=*
//PRINT1   DD   SYSOUT=*
//INPUT1   DD   DSN=BC0THOR.TST.SORTED,DISP=OLD
//OUTPUT1  DD   DSN=BC0THOR.TST.CATALOGD,DISP=(,CATLG),
//         UNIT=DISK,SPACE=(TRK,(15,5),RLSE),
//         DSORG=PS,RECFM=FB,LRECL=80
