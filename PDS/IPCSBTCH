//SCRATCH  EXEC PGM=IDCAMS
//SYSPRINT DD   SYSOUT=*
//SYSIN    DD   *
           DELETE (BC0THOR.TST.IPCS) NONVSAM
//*
//IPCS     EXEC PGM=IKJEFT01,DYNAMNBR=20,REGION=0M
//IPCSDDIR DD   DSN=BC0THOR.IPCSDDIR,DISP=OLD
//SYSPROC  DD   DSN=SYS1.SBLSCLI0,DISP=SHR
//SYSUDUMP DD   SYSOUT=*
//*YSTSPRT DD   SYSOUT=*
//SYSTSPRT DD   DSN=BC0THOR.TST.IPCS,DISP=(,CATLG),
//         UNIT=DISK,SPACE=(CYL,(10,2),RLSE)
//SYSTSIN  DD   *
PROFILE MSGID
IPCS NOPARM
SETDEF DSN('BC0THOR.TST.DUMP18') LIST
SETDEF NOCONFIRM
        STATUS
        SUMMARY FORMAT REGISTERS
SETDEF CONFIRM
END
//EXAMPLES DD   *
        VERBX SYMPTOM
        VERBX LOGDATA
        VERBX VSMDATA 'NOGLOBAL'
