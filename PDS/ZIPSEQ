//BC0THORD JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=4M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//*******************************************************************
//*                 BC0THOR.PDS.JCL(ZIPSEQ)                         *
//* UNZIPS A SEQUENTIAL FILE                                        *
//*******************************************************************
//*
//*        UNZIP (DECOMPRESS) A FILE ALREADY ZIP'D
//*
//*        RECFM, LRECL, AND BLKSIZE MUST BE SPECIFIED
//*        UNIT NAME, VOLSER, AND SPACE PARAMETERS MUST BE SPECIFIED
//*
//*        "ARCHIVE" - NAME OF THE "ZIPPED" FILE ON DASD
//*        "HLQ"     - CAN BE USED TO CHANGE HLQ, IF DESIRED
//*        "OVERWRITE" - WILL CAUSE AN EXISTING FILE TO BE WRITTEN OVER
//*        "ECHO"    - MAY ONLY BE USED WITH V217 TO LIST INPUT CARDS
//*
//PKZIP    EXEC PGM=PKUNZIP,REGION=4M
//*STEPLIB  DD DSN=SPP.PKZIP.LOADLIB,DISP=SHR
//STEPLIB  DD DSN=SPP.MVS.PKZIP.PROD.LOADLIB,DISP=SHR
//SYSPRINT DD SYSOUT=*
//SYSUDUMP DD SYSOUT=*
//SYSIN    DD *
 -ECHO
 -ARCHIVE(BC0TGOL.CBT.FILE001.ZIP)
 -OUTUNIT(DISK)
 -OUTSPACE(TRK)
 -OUTPRIMARY(100)
 -OUTSECONDARY(10)
 -OUTBLKSIZ(27920)
 -OUTLRL(80)
 -OUTTYPE(FB)
 -OUTVOL(SYS004)
 -EXTRACT
 -HLQ(JRC1.JSJ/BC0TGOL.CBT.V416)
 -ZIPCUR(BC0TGOL.CBT.V416.FILE001.DATA)
