//BC0THORE JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=4M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//*********************************************************************
//*                 BC0THOR.PDS.JCL(IPCSDEFN)                       *
//*                 DEFINE AN IPCS DUMP DIRECTORY DATASET             *
//*********************************************************************
//DEFINE   EXEC PGM=IDCAMS
//SYSPRINT DD   SYSOUT=*
//SYSIN    DD   *
 DEFINE CLUSTER -
        (NAME(BC0THOR.IPCSDDIR) -
         VOLUME(TECH02) INDEXED REUSE -
         SHAREOPTIONS(1,3)) -
       DATA -
         (NAME (BC0THOR.IPCSDDIR.DATA) -
         RECORDS(50000,10000) CISZ(4096) -
         KEYS(128,0) RECORDSIZE(384 3072)) -
       INDEX -
         (NAME (BC0THOR.IPCSDDIR.INDEX) -
         RECORDS(10,10) CISZ(2048))
//*
//LISTCAT  EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 LISTCAT ENTRIES(BC0THOR.IPCSDDIR) ALL
