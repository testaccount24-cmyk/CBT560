//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//********************************************************************
//*                  BC0THOR.PDS.JCL(CSECTMRG)                       *
//* READS THE DATASET OUTPUT FROM CSECTLST FOR ALL ENDEVOR LOAD      *
//* LIBRARIES AFTER SORTING IN CSECT-LANGUAGE-DSNAME-MEMBER ORDER    *
//* AND WRITES ON OUTPUT RECORD FOR EACH CSECT THAT IS THE ENTRY     *
//* POINT TO A MEMBER.                                               *
//********************************************************************
//SCRATCH  EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 DELETE BC0THOR.TST.CSECTMRG
 DELETE BC0THOR.TSTSORT.CSECTS
//******************************************************************
//*    SORT RECORDS IN CSECT-LANGUAGE-DSNAME-MEMBER ORDER          *
//******************************************************************
//SORT     EXEC PGM=SORT33,PARM='CORE=MAX,MSG=AP'
//SYSUDUMP DD SYSOUT=(*,,DUMP)
//SORTIN   DD DSN=BC0THOR.TST.ALL.CSECTS,DISP=SHR
//SORTOUT  DD DSN=BC0THOR.TSTSORT.CSECTS,DISP=(,CATLG),
//         UNIT=DISK,SPACE=(TRK,(75,15),RLSE),
//         RECFM=FB,LRECL=80,BLKSIZE=0
//SORTWK01 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK02 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK03 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK04 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK05 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SORTWK06 DD UNIT=DISK,SPACE=(CYL,(5,5))
//SYSOUT   DD SYSOUT=*
//SYSIN    DD *
 SORT FIELDS=(11,8,CH,A,20,3,CH,A,44,3,CH,A,1,8,CH,A)
//******************************************************************
//* WRITE FILE WITH ONE RECORD PER ENTRY POINT CSECT               *
//******************************************************************
//CSECTMRG EXEC PGM=CSECTMRG
//STEPLIB  DD DSN=BC0THOR.LOAD.LOAD,DISP=SHR
//SYSUDUMP DD SYSOUT=*
//ABNLDUMP DD SYSOUT=*
//INPUT1   DD DSN=BC0THOR.TSTSORT.CSECTS,DISP=SHR
//OUTPUT1  DD DSN=BC0THOR.TST.CSECTMRG,DISP=(,CATLG),
//         UNIT=DISK,SPACE=(TRK,(300,150),RLSE),
//         RECFM=VB,LRECL=27962,BLKSIZE=27966
